{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "exclude": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2022", "lib": ["es2022","ES6",
    "DOM"], 
    "noImplicitAny": false,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}